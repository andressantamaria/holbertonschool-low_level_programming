#include "main.h"
/**
 * _abs(int) computes the absolute value of an integer.
 * Return 
 *
 */


int _abs(int n)
{
	if (n > 0)
        {
                return n;
        }
        else if (n == 0)
        {
                return 0;
        }
        else
        {
                return (-1 * n);
        }

        _putchar('\n');
}

